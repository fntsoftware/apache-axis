<?xml version="1.0" ?>
<env:Envelope xmlns:env="http://www.w3.org/2003/05/soap-envelope" 
              xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <env:Body>
    <sb:echoString xmlns:sb="http://soapinterop.org/">
      <inputString xsi:type="xsd:string"><t:value expression="${fn:toUpperCase(inputString)}" type="string" xmlns:t="http://axis.apache.org/mock/template"/></inputString>
    </sb:echoString>
  </env:Body>
</env:Envelope>
